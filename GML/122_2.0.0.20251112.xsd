   <xs:simpleType name="actionOrActivityLabel">
      <xs:annotation>
         <xs:documentation>The action or activity of a vessel.</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Navigating With a Pilot">
            <xs:annotation>
               <xs:documentation>1: Carrying a qualified pilot as part of the vessel navigation team.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Entering Port">
            <xs:annotation>
               <xs:documentation>2: Navigating a vessel into a port.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Leaving Port">
            <xs:annotation>
               <xs:documentation>3: Navigating a vessel out of a port.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Berthing">
            <xs:annotation>
               <xs:documentation>4: A signal station for the control of vessels when berthing.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Slipping">
            <xs:annotation>
               <xs:documentation>5: Detaching a vessel from a wharf or jetty.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Anchoring">
            <xs:annotation>
               <xs:documentation>6: Attaching a vessel to the seabed by means of an anchor and cable.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Weighing Anchor">
            <xs:annotation>
               <xs:documentation>7: Detaching a vessel from the seabed by recovering an anchor and cable.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Transiting">
            <xs:annotation>
               <xs:documentation>8: Navigating a vessel along a route or through a narrow gap, such as under a bridge or through a lock.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Overtaking">
            <xs:annotation>
               <xs:documentation>9: Navigating a vessel past another traveling broadly in the same direction.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Reporting">
            <xs:annotation>
               <xs:documentation>10: Providing details such as the name, location or intentions of a vessel.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Working Cargo">
            <xs:annotation>
               <xs:documentation>11: Loading or unloading cargo.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Landing">
            <xs:annotation>
               <xs:documentation>12: Placing crew or passengers on shore.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Diving">
            <xs:annotation>
               <xs:documentation>13: A signal or message warning of diving activity.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Fishing">
            <xs:annotation>
               <xs:documentation>14: Hunting or catching fish.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Discharging Overboard">
            <xs:annotation>
               <xs:documentation>15: Releasing anything into the sea; often ballast water; or spoil from dredging elsewhere.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Passing">
            <xs:annotation>
               <xs:documentation>16: Navigating a vessel past another travelling broadly in the opposite direction.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Ballast Water Exchange">
            <xs:annotation>
               <xs:documentation>17: Discharge and uptake of ballast water.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Hull Cleaning">
            <xs:annotation>
               <xs:documentation>18: The removal or treatment of biofouling (accumulation of aquatic organisms including microfouling and macrofouling) from a ship's submerged surfaces, including hull and niche areas, conducted either in-water or during dry-docking. The process includes both proactive cleaning (periodic removal of microfouling) and reactive cleaning (removal of micro- and macrofouling as corrective action).</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Scientific Research">
            <xs:annotation>
               <xs:documentation>19: The conduct of observational, sampling, or experimental activities by authorised personnel to collect scientific or environmental data, which may involve the deployment of scientific instruments, collection of biological or geological samples, or in-water survey operations.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Tourism">
            <xs:annotation>
               <xs:documentation>20: Organised recreational visitation and leisure activities in marine areas, including sight-seeing, wildlife observation, glass-bottom vessel tours, and guided nature excursions conducted by commercial or permitted operators.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Education">
            <xs:annotation>
               <xs:documentation>21: Structured activities conducted for training, awareness, or interpretive purposes involving groups or individuals learning about the marine environment, including guided educational programs, school activities, and field instruction conducted within designated marine areas.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Infrastructure Maintenance">
            <xs:annotation>
               <xs:documentation>22: Inspection, repair, or upkeep of existing marine or coastal infrastructure such as wharves, piers, pipelines, moorings, subsea cables, navigational aids, or coastal protection structures, including minor works that do not expand the original footprint.</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
